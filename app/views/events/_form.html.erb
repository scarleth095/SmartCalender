
<%= form_for @event do |f| %>
  <p>
      <%= f.label :date %><br>
      <%= f.text_field :date %>
  </p>

  <p>
      <%= f.label :description %><br>
  <%= f.text_area :description %>
  </p>
  <p>
      <%= f.label :time %><br>
      <%= f.text_field :time %>
  </p> 
  <p>
 
  <%= f.submit %>
  </p>
  <% end %>